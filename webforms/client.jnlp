<?xml version="1.0" encoding="utf-8"?>

<!-- JNLP File for Ganymede Client 

     This file defines the launch characteristics for the
     Ganymede client, allowing the Java Web Start program
     to properly launch the Ganymede client.  This file
     format is documented at 

     http://java.sun.com/products/javawebstart/developers.html

-->

<jnlp spec="1.0+" codebase="#WEBROOT" href="client.jnlp">
  <information>
    <title>Ganymede Client</title>
    <vendor>Applied Research Laboratories</vendor>
    <homepage href="http://www.arlut.utexas.edu/gash2"/>
    <description>Ganymede Client</description>
    <icon href="icon.gif"/>
  </information>
  <resources>
    <j2se version="1.5.0"/>
    <j2se version="1.5.0-beta" href="http://java.sun.com/products/autodl/j2se"/>
    <j2se version="1.5"/>
    <j2se version="1.4.2"/>
    <j2se version="1.4.1"/>
    <j2se version="1.4.1-beta" href="http://java.sun.com/products/autodl/j2se"/>
    <j2se version="1.4"/>
    <j2se version="1.3.1"/>
    <j2se version="1.3"/>
    <j2se version="1.2"/>
    <jar href="ganymede.jar"/>
  </resources>
  <security>
    <j2ee-application-client-permissions/>
  </security>
  <applet-desc 
    documentBase="#WEBROOT"
    name="Ganymede"
    main-class="arlut.csd.ganymede.client.glogin"
    width="320"
    height="350">
    <param name="ganymede.serverhost" value="#SERVERHOST"/>
    <param name="ganymede.registryPort" value="#REGISTRYPORT"/>
  </applet-desc>
</jnlp>
