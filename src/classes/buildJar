#!/bin/sh
#
# Ganymede client jar build script
#
# Version: $Revision: 1.4 $
# Date: $Date: 1999/01/20 04:40:32 $
#
############################################################

currentdir=`pwd`

# Change to the directory where this build script is

prog=$0
builddir=`echo $prog|sed 's%/[^/][^/]*$%%'`
test "x$builddir" = "x$builddir" && builddir=.
cd $builddir

# Load configuration information

. config.sh

# And do the build

echo "Building client jar"

mv -f ganymede.jar ganymede.jar.old

cd client_classes

ln -s ../jdj .

sync_tree

$JAVADIR/jar -cf ../ganymede.jar arlut jdj

cd ..

echo "Done building client jar"